import java.util.*;

public class B {

	public static class NODE {

		double x;
		double y;

		public NODE(double a, double b) {
			x = a;
			y = b;
		}

	}

	public static void main(String[] args) {

		Scanner in = new Scanner(System.in);
		int cases = in.nextInt();
		double dist = 0;
		

		while ((cases--) > 0) {

			in.nextLine();
			double min = 5000000;
			int freckles = in.nextInt();
			ArrayList<NODE> nodes = new ArrayList<NODE>();

			while ((freckles--) > 0) {

				double a = in.nextDouble();
				double b = in.nextDouble();

				nodes.add(new NODE(a, b));

			}
			int location = 1500;
			while (nodes.size() > 1) {
				NODE start = nodes.remove(0);
				
				double temp = 0;
				for (int i = 0; i < nodes.size(); i++) {
					double tempx = nodes.get(i).x;
					double tempy = nodes.get(i).y;

					temp = Math.sqrt(Math.pow(tempy - start.y,2)+ Math.pow(tempx - start.x,2));

					if (temp < min) {
						min = temp;
						location = i;
					}
				}

				dist += min;
				min = 5000000;
			}
			
			System.out.format("%.2f", dist);
			if(cases>0)
				System.out.println();
			dist = 0;
		}

		

	}

}
